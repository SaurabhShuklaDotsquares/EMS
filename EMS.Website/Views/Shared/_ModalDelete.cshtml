@model EMS.Web.Models.Others.Modal
<div id="@(string.IsNullOrEmpty(Model.ElementId)?"frm-delete":Model.ElementId)">
    @using (Html.BeginForm())
    {
        @Html.Partial("_ModalHeader", Model.Header)
        <div id="validation-summary"></div>
        <div class="modal-body">
            <p class="del-msg">@Html.Raw(Model.Message)</p>
        </div>
        @Html.Partial("_ModalFooter", Model.Footer)
    }
</div>